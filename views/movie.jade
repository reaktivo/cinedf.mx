extends layout

append content
  #movie
    if movie.embed
      .video-container: iframe(src=movie.embed)
    else
      img(src=movie.img.path)
    .text
      h1= movie.title
      != movie.synopsis

append scripts
  script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js")
  != js('movie')